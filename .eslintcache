n    }\r\n  }\r\n\r\n  return (\r\n    <div className=\"container mt-5 mb-5\">\r\n      {information\r\n        ? <div>\r\n          <div className=\"row mt-5\">\r\n            <div className=\"col-6\">\r\n              <select className=\"custom-select p-2\" name=\"search\" id=\"search\" onChange={(e) => setName(e.target.value)} >\r\n                {columns.map((title) => {\r\n                  return (<option value={title.dataField}>{title.dataField}</option>)\r\n                })}\r\n              </select>\r\n\r\n            </div>\r\n            <div className=\"col-6\">\r\n              <div className=\"d-flex\">\r\n                <input className=\"form-control\" type=\"textarea\" name=\"exact\" onChange={(e) => handlefilterData(e.target.value)}></input>\r\n              </div>\r\n            </div>\r\n\r\n\r\n          </div>\r\n\r\n\r\n          <div className=\"d-flex justify-content-center mt-5\">\r\n            <h1 style={{ color: \"#1dd81a\", border: \" 1px solid #1dd81a\", padding: '1rem', width: '100%', backgroundColor: \"#c0ffc4\", textAlign: 'center' }}>{title}</h1>\r\n          </div>\r\n          <BootstrapTable\r\n            hover\r\n            keyField=\"name\"\r\n            data={filterData}\r\n            columns={columns}\r\n            pagination={paginationFactory()}\r\n            rowEvents={rowEvents} />\r\n\r\n\r\n        </div>\r\n        : <div class=\"container\">\r\n          <div class=\"d-flex text-center justify-content-center flex-column align-items-center mt-5 p-5\">\r\n            <h3>Loading</h3>\r\n            <div class=\"spinner-border mt-5\" role=\"status\">\r\n\r\n              <span class=\"sr-only\"></span>\r\n            </div>\r\n          </div>\r\n        </div>}\r\n    </div>\r\n  );\r\n}\r\n\r\n\r\n\r\n\r\n\r\n","C:\\Users\\Luz Testai\\Desktop\\Proyectos Bowery\\bowery-studio\\my-react\\src\\views\\IndividualComments\\individualComments.jsx",["102","103"],"C:\\Users\\Luz Testai\\Desktop\\Proyectos Bowery\\bo